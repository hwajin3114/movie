<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorators">
	<decorator name="all-theme" page="all-theme.jsp">
		<pattern>/*</pattern>
	</decorator>
	<decorator name="reservation-theme"
		page="reservation-theme.jsp">
		<pattern>/reservation/*</pattern>
	</decorator>
	<!-- Excludes will never be decorated by Sitemesh. -->
	<excludes>
		<pattern>*/ajax/*</pattern>
		<pattern>/data/plaintext/*</pattern>
		<pattern>*/exclude/*</pattern>
	</excludes>
</decorators>