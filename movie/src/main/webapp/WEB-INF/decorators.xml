<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/decorators">
	<decorator name="all-theme" page="all-theme.jsp">
		<pattern>/*</pattern>
	</decorator>
	<decorator name="reservation-theme" page="reservation-theme.jsp">
		<pattern>/reservation/*</pattern>
	</decorator>
		<decorator name="user-theme" page="user-theme.jsp">
		<pattern>/member/memberInfo.do</pattern>
		<!-- <pattern>/예매/예매내역.jsp</pattern> -->
	</decorator>
	<!-- Excludes will never be decorated by Sitemesh. -->
	<excludes>
		<pattern>*/ajax/*</pattern>
		<pattern>/data/plaintext/*</pattern>
		<!-- <pattern>/index.jsp</pattern> -->
	</excludes>
</decorators>